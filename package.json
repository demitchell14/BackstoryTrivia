{
  "name": "backstorytrivia-server",
  "version": "0.1.0",
  "private": true,
  "description": "Websocket enabled trivia games",
  "main": "./dist/www/server.js",
  "scripts": {
    "start": "set NODE_ENV=production & node ./dist/www/server.js",
    "run-server": "nodemon --inspect --watch dist",
    "watch-ts": "tsc --watch",
    "build-ts": "tsc -b",
    "e2e": "repo-tools test deploy",
    "test": "repo-tools test app",
    "cover": "nyc --cache npm test; nyc report --reporter=html",
    "deploy": "gcloud app deploy -q"
  },
  "browserslist": [
    "last 4 version"
  ],
  "cloud-repo-tools": {
    "requiresKeyFile": true,
    "requiresProjectId": true,
    "test": {
      "app": {
        "args": [
          "www/server.js"
        ],
        "msg": "Hello, world!!!"
      }
    }
  },
  "semistandard": {
    "globals": [
      "after",
      "afterEach",
      "before",
      "beforeEach",
      "describe",
      "it"
    ]
  },
  "author": "Devin Mitchell",
  "license": "ISC",
  "dependencies": {
    "@types/debug": "0.0.31",
    "bcrypt": "^3.0.2",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "debug": "^4.1.0",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "express-session": "latest",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "mongodb": "^3.1.9",
    "morgan": "^1.9.1",
    "socket.io": "^2.1.1",
    "typescript": "^3.1.6",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@types/bcrypt": "^3.0.0",
    "@types/bootstrap": "^4.1.2",
    "@types/compression": "0.0.36",
    "@types/express": "^4.16.0",
    "@types/lodash": "^4.14.118",
    "@types/mongodb": "^3.1.14",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.12.5",
    "@types/popper.js": "^1.11.0",
    "@types/uuid": "^3.4.4",
    "babel-loader": "^8.0.4"
  },
  "engines": {
    "node": ">=10"
  }
}
